unit uCliente.Controller;

interface

uses
  uCliente;

type
  TClienteController = class
  private
    FCliente: TCliente;

  public
  property Cliente: TCliente read FCliente write FCliente;
  constructor Create;
  end;

implementation

{ TClienteController }

constructor TClienteController.Create;
begin
  FCliente:=TCliente.Create;
end;

end.
